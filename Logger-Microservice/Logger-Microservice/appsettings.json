{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    //Some formatting to the appsettings, making it much easier to see what happened.
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "dd.MM.yyyy HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  }
}